type vector (x: double default 0.0, y: double default 0.0) {
    fn add(another: ptr(vector)) -> void {
        this.x = this.x + another.x
        this.y = this.y + another.y
    }

    fn div(d: double) -> void {
        this.x = this.x / d
        this.y = this.y / d
    }

    fn magnitude -> double {
        resolve sqrt(this.x * this.x + this.y * this.y)
    }
}